<h1>Listing activity_logs</h1>

<table>
  <thead>
    <tr>
      <th>Item type</th>
      <th>Item</th>
      <th>Act action</th>
      <th>Updated by</th>
      <th>Activity</th>
      <th>Act tstamp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @activity_logs.each do |activity_log| %>
      <tr>
        <td><%= activity_log.item_type %></td>
        <td><%= activity_log.item_id %></td>
        <td><%= activity_log.act_action %></td>
        <td><%= activity_log.updated_by %></td>
        <td><%= activity_log.activity %></td>
        <td><%= activity_log.act_tstamp %></td>
        <td><%= link_to 'Show', activity_log %></td>
        <td><%= link_to 'Edit', edit_activity_log_path(activity_log) %></td>
        <td><%= link_to 'Destroy', activity_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Activity log', new_activity_log_path %>
